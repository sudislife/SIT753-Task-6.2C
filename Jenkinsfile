pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo 'Building the app'
            }
        }
        stage('Unit and Integration Tests'){
            steps{
                echo 'Testing the app'
            }
        }
        stage('Code Analysis'){
            steps{
                echo 'Analysis'
            }
        }
        stage('Security Scan'){
            steps{
                echo 'Scanning the app'
            }
        }
        stage('Deploy to Staging'){
            steps{
                echo 'Deploying the app'
            }
        }
        stage('Integration Tests on Staging'){
            steps{
                echo 'Testing the app'
            }
        }
        stage('Deploy to Production'){
            steps{
                echo 'Deploying the app'
            }
        }
    }
}